<Button_main@MDButton>
#    on_release: root.parent.parent.parent.choice_button(self)
    theme_bg_color: "Custom"
    # theme_color: "Custom"
    md_bg_color: .1,.1,.1,1
    style: "outlined"
    radius:5
    # md_bg_color: "red"
    # md_bg_color: "green"
    # style: "elevated" 'md_bg_color_disabled' 'on_md_bg_color'
    # style: "filled"
    # style: "tonal"
    # style: "text"
    pos_hint: {"bottom": 1}
    theme_width: "Custom"
    theme_height: "Custom"
    size_hint: (1,.9)
<Button_text@MDButtonText>
    pos_hint: {"center_x": 1,"center_y": 1}
    theme_text_color: "Custom"
    text_color: "white"
    theme_font_size: "Custom"
    font_size: sp(20)
<TimeBox@MDBoxLayout>
    orientation: "vertical"
    padding: 5
    canvas.before:
        Color:
            rgba: 0,0,0,1
        Line:
            width:1
            rectangle: self.x,self.y,self.width,self.height
<TimeSpinner@Spinner>
    size_hint: 1, 1
    # pos_hint: {"right":1,"top":1}
    font_size: 50

<TitleLabel@MDLabel>
    size_hint: 1, 1
    # font_size: 30
    color: 0,0,0
    font_style: "Title"
    role: "small"
    bold: True
    text_size: self.size
    halign: "center"
    valign: "center"

    # theme_font_size: "Custom"

<Pages_main>:
    id: page_main
    name: "main_page"
    md_bg_color: app.theme_cls.primaryContainerColor # app.theme_cls.primaryColor # app.theme_cls.primaryContainerColor
    MDBoxLayout:
        id: root_main_box
        orientation: "vertical"
        size: root.size
        MDBoxLayout:
            id: TOP_box
            padding: [sp(10),sp(10),sp(10),0]
            size_hint_y: .8
            MDBoxLayout:
                id:left_box
                orientation: "vertical"
                MDBoxLayout:
                    size_hint_y: .2
                    padding: [0,0,sp(5),sp(5)]
                    orientation: "vertical"
                    MDBoxLayout:
                        size_hint_y: .6
                        TitleLabel:
                            text: "Дата"
                        TimeSpinner:
                            id: spinner_day
                            text: root.day_spinner
                            values: map(str,list(range(1,32)))
                            on_text: pass
                    MDBoxLayout:
                        size_hint_y: .4
                        TimeSpinner:
                            id: spinner_month
                            text:  root.month_spinner_str
                            values: root.month_spinner_lst
                            font_size: 30
                            on_text: pass
                TimeBox:
                    size_hint_y: .2
                    MDBoxLayout:
                        id: lunch_time_box
                        size_hint: 1,1
                        TitleLabel:
                            text: "     установите \nНАЧАЛО ОБЕДА"
                    MDBoxLayout:
                        TimeSpinner:
                            id: hoursstartlunch
                            text: root.hours_start_lunch
                            values: [str(x) for x in range(1,25)]
                            on_text: root.start_calculate_work_time()
                        TitleLabel:
                            size_hint: .1, 1
                            text: ":"
                        TimeSpinner:
                            id: minutesstartlunch
                            text: root.minutes_start_lunch
                            values: [str(x).zfill(2) for x in range(0,60)]
                            on_text: root.start_calculate_work_time()
                TimeBox:
                    size_hint_y: .2
                    MDBoxLayout:
                        orientation: "vertical"
                        padding:[20,10,20,0]
                        TitleLabel:
                            text: "    установите \nКОНЕЦ ОБЕДА"

                    MDBoxLayout:
                        TimeSpinner:
                            id: hoursendlunch
                            text: root.hours_end_lunch
                            values: [str(x) for x in range(1,25)]
                            on_text: root.start_calculate_work_time()
                        TitleLabel:
                            size_hint: .1, 1
                            text: ":"
                        TimeSpinner:
                            id: minutesendlunch
                            text: root.minutes_end_lunch
                            values: [str(x).zfill(2) for x in range(0,60)]
                            on_text: root.start_calculate_work_time()
                TimeBox:
                    size_hint_y: .3
                    MDBoxLayout:
                        size_hint_y: .15
                        TitleLabel:
                            id: root.date_a_day
                            text: "9 Декабрь"
                            role: "large"
                    MDBoxLayout:
                        size_hint_y: .15
                        TitleLabel:
                            text: root.lab_save_txt
                            role: "large"
                            halign: "left"
                    MDBoxLayout:
                        size_hint_y: .25
                        TitleLabel:
                            size_hint_x: .5
                            text: root.total_hours_work
                            font_style: "Headline"
                            role: "large"
                        TitleLabel:
                            text: "часов"
                            bold: False
                            halign: "left"

                    MDBoxLayout:
                        size_hint_y: .25
                        TitleLabel:
                            text: root.total_minutes_work
                            font_style: "Headline"
                            role: "large"
                            halign: "right"
                        TitleLabel:
                            size_hint_x: .5
                            text: "мин."
                            bold: False



            MDBoxLayout:
                id: right_box
                orientation: "vertical"

                TimeBox:
                    padding: 5
                    MDBoxLayout:
                        TitleLabel:
                            text: "установите НАЧАЛО\n      рабочего дня"
                    MDBoxLayout:
                        TimeSpinner:
                            id: startworkhours
                            text: root.hours_start_work
                            values: [str(x) for x in range(1,25)]
                            on_text: root.start_calculate_work_time()
                        TitleLabel:
                            size_hint: .1, 1
                            text: ":"
                        TimeSpinner:
                            id: startworkminutes
                            text: root.minutes_start_work
                            values: [str(x).zfill(2) for x in range(0,60)]
                            on_text: root.start_calculate_work_time()
                TimeBox:
                    padding: 5
                    orientation: "vertical"
                    MDBoxLayout:
                        id: box_time_end
                        TitleLabel:
                            text: "установите КОНЕЦ\n      рабочего дня"
                    MDBoxLayout:
                        TimeSpinner:
                            id: hoursendwork
                            text: root.hours_end_work
                            values: [str(x) for x in range(1,25)]
                            on_text: root.start_calculate_work_time()

                        TitleLabel:
                            size_hint: .1, 1
                            text: ":"
                        TimeSpinner:
                            id: minutesendwork
                            text: root.minutes_end_work
                            values: [str(x).zfill(2) for x in range(0,60)]
                            on_text: root.start_calculate_work_time()
                TimeBox:
                    padding: 5
                    orientation: "vertical"
                    MDBoxLayout:
                        Button:
                            size_hint:1,1
                            font_size: 25
                            # on_press: root.btn_weekend()
                            text: "Выходной"
                    MDBoxLayout:
                        orientation: "vertical"
                        size_hint:1,1
                        TitleLabel:
                            text: "запомнить время"
                            font_style: "Body"
                            role: "small"
                        CheckBox:####################################################################################
                            id: check_save_time
                            color: 0,0,0
                            size_hint_x: .5
                            # on_active: root.remember_the_time(self.active)
                TimeBox:
                    orientation: "vertical"
                    RouteTextInput:
                        id: route_number_textinput
                        text: ""
                        mode: "outlined"
                        multiline: False
                        font_size: 40
                        MDTextFieldHintText:
                            text: "Маршрут"
                            text_color_normal: "brown"


                    KartaTextInput:
                        id: karta_route_number_textinput
                        text: ""
                        mode: "outlined"
                        multiline: False
                        font_size: 40
                        MDTextFieldHintText:
                            text: "Карта"
                            text_color_normal: "brown"






        MDBoxLayout:
            id: BOTTOM_box
            padding: [sp(10),sp(10),sp(10),0]
            orientation: "vertical"
            size_hint_y: .15
            MDBoxLayout:
                size_hint_y: .40
                padding:[0,0,0,sp(5)]
                MDBoxLayout:
                    padding:[0,0,sp(10),0]
                    Button_main:
                        on_release: root.intercept_data_main_screen()
                        Button_text:
                            text: "Сохранить"
                MDBoxLayout:
                    Button_main:
                        on_release: app.quit_program()
                        Button_text:
                            pos_hint: {"right": .7,"center_y": .5}
                            text: "Выход"
            MDBoxLayout:
                size_hint:(1,.1)
                # padding: sp(5)
                TitleLabel:
                    markup: True
                    text: "автор: Погодаев Илья Сергеевич [color=#0B0080][b]ilya1880@mail.ru[/b][/color]"
                    theme_font_size: "Custom"
                    font_size: sp(11)
                    halign: "center"
                    valign: "center"
<Pages_stat>:
    id: page_stat
    name: "stat_page"
    md_bg_color: app.theme_cls.primaryContainerColor # app.theme_cls.primaryColor # app.theme_cls.primaryContainerColor
    MDBoxLayout:
        id: root_stat_box
        orientation: "vertical"
        MDBoxLayout:
            padding: [sp(10),sp(10),sp(10),0]
            size_hint_y: .25
            TimeSpinner:
                id: spinner_day
                size_hint: .3, 1
                text: "27"# "root.day_spinner_str"
                values: map(str,list(range(1,32)))
                on_text: root.create_a_date_for_label()

            TimeSpinner:
                size_hint: .5, 1
                id: spinner_month
                font_size: 30
                text:  "Сентябрь"# "root.month_spinner_str"
                # values: root.spinner_month_lst
                on_text: root.create_a_date_for_label()

            TimeSpinner:
                id: spinner_day
                size_hint: .3, 1
                # pos_hint: {"top":1}
                text: "27"# "root.day_spinner_str"
                values: map(str,list(range(1,32)))
                on_text: root.create_a_date_for_label()
        MDBoxLayout:
            id: TwoBoxStat
            size_hint: 1, 1
            # md_bg_color: .1,1,.1
            padding: sp(10)
            MDCard:
                orientation: "vertical"
                size_hint: 1, 1
                # elevation: 20
                MDLabel:
                    text_size: self.size
                    theme_font_size: "Custom"
                    font_size: sp(20)
                    bold:True
                    padding: sp(10)
                    size_hint: 1,.06
                    text: "Месяц"
                    valign: "center"
                    halign: "center"
                MDLabel:
                    id: card_label_id
                    text_size: self.size
                    theme_font_size: "Custom"
                    font_size: sp(15)
                    bold:True
                    spacing: sp(10)
                    padding: sp(10)
                    size_hint: 1,1
                    text:  "root.strig_statistic_for_label"
                    valign: "top"

        MDBoxLayout:
            Button:
                text: "Кнопка"
        MDBoxLayout:
            size_hint_y: .5




